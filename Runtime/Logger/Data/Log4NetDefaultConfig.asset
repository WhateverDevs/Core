  m_Script: {fileID: 11500000, guid: 7ecde6843dbb4e37844aac88b3a531dc, type: 3}
  m_Name: Log4NetDefaultConfig
  m_EditorClassIdentifier: 
  DefaultConfig: "<?xml version=\"1.0\" encoding=\"utf-8\"?>\r\n<log4net>\r\n   
    <appender name=\"UnityDebugAppender\" type=\"WhateverDevs.Core.Runtime.Logger.Appenders.UnityDebugAppender,WhateverDevs.Core.Runtime\">\r\n       
    <layout type=\"log4net.Layout.PatternLayout,log4net\">\r\n            <param
    name=\"ConversionPattern\" value=\"%d{ABSOLUTE} %-5p %c{1}:%L - %m%n\"/>\r\n       
    </layout>\r\n    </appender>\r\n    <appender name=\"Logfile\" type=\"log4net.Appender.FileAppender,log4net\">\r\n\t\t<file
    type=\"log4net.Util.PatternString,log4net\">\r\n\t\t\t<converter>\r\n\t\t\t\t<!--
    Tell the configurator that this is the converter for \"%persistentDataStore\"
    below -->\r\n\t\t\t\t<name value=\"persistentDataStore\" />\r\n\t\t\t\t<!-- Tell
    the converter what to call to do the conversion -->\r\n\t\t\t\t<type value=\"WhateverDevs.Core.Runtime.Logger.PersistentDataStoreConverter,
    WhateverDevs.Core.Runtime\" />\r\n\t\t\t</converter>\r\n\t\t\t<conversionPattern
    value=\"%persistentDataStore/logs/logfile-%date{yyyy-MM-dd-HH-mm-ss}.log\" />\r\n\t\t</file>\r\n\t\t<appendToFile
    value=\"true\" />\r\n\t\t<rollingStyle value=\"Date\" />\r\n\t\t<datePattern
    value=\"yyyyMMdd\" />\r\n        <layout type=\"log4net.Layout.PatternLayout,log4net\">\r\n           
    <param name=\"ConversionPattern\" value=\"%d{ABSOLUTE} %-5p %c{1}:%L - %m%n\"/>\r\n\r\n       
    </layout>\r\n    </appender>\r\n    <root>\r\n        <level value=\"ALL\"/>\r\n       
    <appender-ref ref=\"UnityDebugAppender\"/>\r\n        <appender-ref ref=\"Logfile\"/>\r\n   
    </root>\r\n</log4net>"
  AlwaysOverride: 0
